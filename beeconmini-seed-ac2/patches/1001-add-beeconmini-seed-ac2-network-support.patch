# 文件: 1001-add-beeconmini-seed-ac2-network-support-fixed.patch
From: BeeconMini AC2 Support <support@beeconmini.com>
Date: Mon, 01 Jan 2024 00:00:00 +0800
Subject: [PATCH] add beeconmini seed ac2 network support

--- a/target/linux/mediatek/filogic/base-files/etc/board.d/02_network
+++ b/target/linux/mediatek/filogic/base-files/etc/board.d/02_network
@@ -90,7 +90,8 @@
 	bananapi,bpi-r3-mini|\
 	edgecore,eap111|\
 	huasifei,wh3000-emmc|\
-	huasifei,wh3000-pro)
+	huasifei,wh3000-pro|\
+	beeconmini,seed-ac2)
 		ucidef_set_interfaces_lan_wan eth0 eth1
 		;;
 	bananapi,bpi-r4|\
@@ -258,4 +258,8 @@
 	yuncore,ax835)
 		label_mac=$(mtd_get_mac_binary "Factory" 0x4)
 		;;
+	beeconmini,seed-ac2)
+		lan_mac=$(mtd_get_mac_binary "art" 0x0)
+		wan_mac=$(macaddr_add "$lan_mac" 1)
+		;;
 	esac
